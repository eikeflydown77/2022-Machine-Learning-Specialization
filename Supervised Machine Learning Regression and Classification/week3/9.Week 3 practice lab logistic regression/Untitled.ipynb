   "source": [
    "from sympy import *\n",
    "x,y,z = symbols('x,y,z')"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 19,
   "id": "fb6c9b5b",
   "metadata": {},
   "outputs": [],
   "source": [
    "eq1 = 0.3*x + 0.4*y + 0.3*z - x\n",
    "eq2 = 0.2*x + 0.2*y + 0.6*z - y\n",
    "eq3 = 0.1*x + 0.8*y + 0.1*z - z\n",
    "eq4 = x + y + z - 1"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 20,
   "id": "496af4f1",
   "metadata": {},
   "outputs": [],
   "source": [
    "result = solve([eq1,eq2,eq3,eq4],[x,y,z])"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 21,
   "id": "32d78cad",
   "metadata": {},
   "outputs": [
